<?xml version="1.0"?>
<launch>

    <arg name="id" default="1" />
    <arg name="output" default="log" />

    <!-- Flocking tracking algorithm -->
    <node pkg="uav_flocking" type="uav_flocking_tracking" name="uav_tracking_$(arg id)" output="$(arg output)" required="true">
        <rosparam file="$(find uav_flocking)/param/uav_flocking.yaml" command="load" />
        <rosparam file="$(find swarm_behaviors_position)/param/position.yaml" command="load" />
        <rosparam file="$(find swarm_behaviors_velocity)/param/velocity.yaml" command="load" />
    </node>

</launch>
